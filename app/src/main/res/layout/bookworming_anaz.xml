<?xml version="1.0" encoding="utf-8"?>
<!-- Bookworming anazitisi-->
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageButton
        android:id="@+id/imageButton3"
        style="@style/Widget.AppCompat.ImageButton"
        android:layout_width="46dp"
        android:layout_height="32dp"
        android:background="#FFFFFF"
        android:baselineAlignBottom="true"
        android:clickable="true"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/basket"
        tools:srcCompat="@drawable/basket"
        tools:visibility="visible" />

    <Spinner
        android:id="@+id/spinner2"
        android:layout_width="30dp"
        android:layout_height="25dp"
        android:background="#0C0C15"
        android:foreground="@drawable/spinner"
        android:isScrollContainer="false"
        android:longClickable="false"
        android:scrollbarTrackVertical="@drawable/spinner"
        android:spinnerMode="dropdown"
        android:splitMotionEvents="true"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/imageButton"
        style="@style/Widget.AppCompat.ImageButton"
        android:layout_width="46dp"
        android:layout_height="47dp"
        android:background="#FFFFFF"
        android:longClickable="true"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/eidop"
        tools:srcCompat="@drawable/eidop"
        tools:visibility="visible" />

    <ImageButton
        android:id="@+id/agorabutton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_marginEnd="112dp"
        android:layout_marginRight="112dp"
        android:background="@null"
        android:clickable="true"
        android:onClick="clickNew"
        android:src="@drawable/agora_icon"
        android:text="@string/mybutton"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:shapeAppearanceOverlay="@style/ShapeAppearanceOverlay.MyApp.Button.Rounded" />

    <ImageButton
        android:id="@+id/daneismosbutton"
        android:layout_width="52dp"
        android:layout_height="36dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_marginEnd="260dp"
        android:layout_marginRight="260dp"
        android:background="@null"
        android:clickable="true"
        android:onClick="clickNew"
        android:src="@drawable/daneismos_icon"
        android:text="@string/mybutton"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:shapeAppearanceOverlay="@style/ShapeAppearanceOverlay.MyApp.Button.Rounded" />

    <Button
        android:id="@+id/profile_button"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:background="@drawable/button_bg_round"
        android:padding="0dp"
        android:text=""
        tools:layout_editor_absoluteX="371dp"
        tools:layout_editor_absoluteY="0dp" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="143dp"
        android:layout_height="121dp"
        android:layout_marginTop="196dp"
        android:contentDescription="@android:string/untitled"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/logo" />

    <EditText
        android:id="@+id/edit_text"
        android:layout_width="130dp"
        android:layout_height="45dp"
        android:hint="type a category"
        android:inputType="textPersonName|textCapWords"
        tools:layout_editor_absoluteX="140dp"
        tools:layout_editor_absoluteY="380dp" />

    <SearchView
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:layout_marginStart="92dp"
        android:layout_marginLeft="92dp"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="380dp" >

    </SearchView>

    <TextView
        android:id="@+id/textView"

        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:text="BOOKWORMING"
        android:textSize="20dp"
        android:textStyle="bold|italic"
        tools:layout_editor_absoluteX="140dp"
        tools:layout_editor_absoluteY="334dp" />

</androidx.constraintlayout.widget.ConstraintLayout>